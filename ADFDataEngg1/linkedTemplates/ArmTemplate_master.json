{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFDataEngg1"
		},
		"Sftp1_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp1_LS'"
		},
		"Sftp1_LS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "test.rebex.net"
		},
		"Sftp1_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "demo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFDataEngg1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Sftp1_LS_password": {
						"value": "[parameters('Sftp1_LS_password')]"
					},
					"Sftp1_LS_properties_typeProperties_host": {
						"value": "[parameters('Sftp1_LS_properties_typeProperties_host')]"
					},
					"Sftp1_LS_properties_typeProperties_userName": {
						"value": "[parameters('Sftp1_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}